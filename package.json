{
  "name": "ale-world",
  "description": "Personal and Professional Website",
  "version": "0.0.1",
  "private": true,
  "author": "Ale Figueroa <me@ale.world>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "es6",
    "hmr",
    "hot",
    "babel",
    "webpack",
    "sentry",
    "architecture",
    "webpack-dev-middleware",
    "webpack-hot-middleware",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yknx4/aleworld.git"
  },
  "bugs": "https://github.com/yknx4/aleworld/issues",
  "scripts": {
    "clean:build": "rimraf build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "npm run clean:build",
    "start": "webpack-dashboard -c cyan -- node dev-server.js",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.js --progress",
    "postbuild": "npm run build:html",
    "build:serve": "http-server build/client -p 3003 --cors -o",
    "lint": "eslint config src/js/** --ext .js",
    "jscs": "jscs src/js/",
    "typecheck": "flow",
    "test": "node scripts/test.js --env=jsdom",
    "tunnel": "lt --port 3000",
    "deploy": "node scripts/deploy.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "6.5.0",
    "bootstrap-sass": "^3.3.7",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "command-line-args": "^4.0.2",
    "copy-webpack-plugin": "3.0.1",
    "cross-env": "2.0.1",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-react": "6.2.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "0.9.0",
    "flow-bin": "^0.42.0",
    "git-branch": "^0.3.0",
    "http-server": "0.9.0",
    "imports-loader": "^0.7.1",
    "jest": "^19.0.2",
    "jest-enzyme": "^3.0.0",
    "jquery": "^3.2.1",
    "jscs": "3.0.7",
    "jsdom": "9.4.5",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "0.11.1",
    "react-addons-test-utils": "^15.4.2",
    "redbox-react": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-slider-monitor": "1.0.7",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.7",
    "style-loader": "^0.14.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "0.14.1"
  },
  "dependencies": {
    "autoprefixer": "6.4.1",
    "babel-preset-node6": "^11.0.0",
    "babel-runtime": "6.11.6",
    "bootstrap-loader": "^2.0.0",
    "classnames": "2.2.5",
    "contentful": "^4.1.2",
    "dotenv": "^4.0.0",
    "enzyme": "^2.7.1",
    "express": "4.14.0",
    "global": "^4.3.1",
    "image-webpack-loader": "^3.3.0",
    "lodash": "4.15.0",
    "numeral": "^2.0.4",
    "raven": "^1.2.0",
    "raven-js": "3.6.1",
    "react": "15.3.1",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.5.0",
    "react-ga": "^2.1.2",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "3.0.0-beta.2",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "react-test-renderer": "^15.4.2",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-promise": "0.5.3",
    "reselect": "2.5.3",
    "sass-resources-loader": "^1.2.0",
    "socket.io-client": "^1.7.3",
    "svgexport": "^0.3.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src/client/scripts",
      "./src/client/assets",
      "./src/client/assets/javascripts"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
